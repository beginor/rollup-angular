{
  "name": "rollup-starter",
  "version": "1.0.0",
  "description": "rollup starter kit",
  "main": "index.html",
  "scripts": {
    "build": "npm run clean && npm run rollup && npm run copy",
    "clean": "rm -rf dist && mkdir dist",
    "copy": "cp src/index.html dist/index.html",
    "browser-sync": "browser-sync start --config bs-config.js",
    "rollup": "rollup -c rollup.config.js",
    "rollup-watch": "npm run rollup -- --watch",
    "start": "npm run clean && npm run copy && concurrently \"npm run browser-sync\" \"npm run rollup-watch\"",
    "test": "test",
    "ng": "NG_PERSISTENT_BUILD_CACHE=1 ng",
    "build-all": "npm run build-shared && npm run build-web && npm run build-handset",
    "build-shared": "NG_PERSISTENT_BUILD_CACHE=1 ng build app-shared --configuration production",
    "build-web": "NG_PERSISTENT_BUILD_CACHE=1 ng build web --configuration production --base-href=/net-core-app/web/",
    "build-handset": "NG_PERSISTENT_BUILD_CACHE=1 ng build handset --configuration production --base-href=/net-core-app/handset/",
    "watch-web": "NG_PERSISTENT_BUILD_CACHE=1 ng build web --watch --configuration development --base-href=/net-core-app/web/",
    "watch-handset": "NG_PERSISTENT_BUILD_CACHE=1 ng build handset --watch --configuration development --base-href=/net-core-app/handset/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/rollup-starter.git"
  },
  "keywords": [
    "rollup",
    "starter"
  ],
  "author": "beginor <beginor@qq.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beginor/rollup-starter/issues"
  },
  "homepage": "https://github.com/beginor/rollup-starter#readme",
  "dependencies": {
    "@angular/animations": "~12.1.0",
    "@angular/common": "~12.1.0",
    "@angular/compiler": "~12.1.0",
    "@angular/core": "~12.1.0",
    "@angular/forms": "~12.1.0",
    "@angular/platform-browser": "~12.1.0",
    "@angular/platform-browser-dynamic": "~12.1.0",
    "@angular/router": "~12.1.0",
    "@angular/cdk": "~12.1.0",
    "@angular/material": "~12.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "rollup": "^2.51.1",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.2",
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "~12.1.0",
    "ng-packagr": "^12.1.0"
  }
}
