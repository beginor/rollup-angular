{
  "name": "rollup-starter",
  "version": "1.0.0",
  "description": "rollup starter kit",
  "main": "index.html",
  "scripts": {
    "build": "npm run clean && npm run rollup && npm run copy-libs",
    "build-app": "npm run clean-app && npm run ngc-web && npm run rollup",
    "clean": "rm -rf dist && mkdir dist",
    "clean-app": "rm -rf dist/main.* dist/chunks",
    "copy-libs": "./copy-libs.sh",
    "browser-sync": "browser-sync start --config bs-config.js",
    "rollup": "rollup -c rollup.config.js",
    "rollup-watch": "npm run rollup -- --watch",
    "dev": "npm run clean-app && concurrently 'npm run browser-sync' 'npm run rollup-watch' 'npm run ngc-web-watch'",
    "start": "npm run clean && npm run copy-libs && npm run dev",
    "test": "echo test",
    "ngcc": "NG_PERSISTENT_BUILD_CACHE=1 ngcc",
    "ngc-web": "NG_PERSISTENT_BUILD_CACHE=1 ngc -p tsconfig.app.json",
    "ngc-web-watch": "npm run ngc-web -- -w",
    "watch-web": "concurrently 'npm run ngc-web-watch' 'npm run rollup-watch'",
    "ng": "NG_PERSISTENT_BUILD_CACHE=1 ng",
    "build-shared": "NG_PERSISTENT_BUILD_CACHE=1 ng build app-shared --configuration development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/rollup-starter.git"
  },
  "keywords": [
    "rollup",
    "starter"
  ],
  "author": "beginor <beginor@qq.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beginor/rollup-starter/issues"
  },
  "homepage": "https://github.com/beginor/rollup-starter#readme",
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.6.0",
    "tslib": "^2.3.1",
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@angular/cdk": "~12.2.0",
    "@angular/material": "~12.2.0",
    "rxjs": "^6.6.0",
    "zone.js": "~0.11.4",
    "@angular/localize": "~12.2.0",
    "devextreme": "^21.1.6",
    "devextreme-angular": "^21.1.6",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0-beta.2",
    "ng-zorro-antd": "^12.0.1"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.1.6",
    "eslint": "^8.0.1",
    "@typescript-eslint/parser": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "rollup": "^2.58.0",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "sass": "^1.43.2",
    "typescript": "^4.4.4",
    "esbuild": "^0.13.6",
    "@angular-devkit/build-angular": "~12.2.9",
    "@angular/cli": "~12.2.9",
    "@angular/compiler-cli": "~12.2.9",
    "ng-packagr": "^12.2.0",
    "@types/node": "^12.11.1"
  }
}
