{
  "name": "@beginor/rollup-angular",
  "version": "1.0.0",
  "description": "rollup angular starter kit",
  "main": "index.html",
  "scripts": {
    "build": "pnpm run clean && pnpm run rollup  && pnpm run sass",
    "build-app": "pnpm run clean-app && pnpm run ngc-web && pnpm run rollup && pnpm run sass",
    "clean": "rm -rf dist && mkdir dist",
    "clean-app": "rm -rf dist/main.* dist/chunks",
    "browser-sync": "browser-sync start --config bs-config.js",
    "rollup": "rollup -c rollup.config.js",
    "rollup-watch": "pnpm run rollup --watch",
    "sass": "sass styles:dist/styles",
    "sass-watch": "pnpm run sass -w",
    "dev": "pnpm run clean-app && concurrently 'pnpm run browser-sync' 'pnpm run watch-web'",
    "start": "pnpm run clean && pnpm run dev",
    "test": "echo test",
    "ngcc": "ngcc",
    "ngc-web": "ngc -p projects/web/tsconfig.app.json",
    "ngc-web-watch": "pnpm run ngc-web -w",
    "watch-web": "concurrently 'pnpm run ngc-web-watch' 'pnpm run rollup-watch'",
    "ng": "ng",
    "build-shared": "ng build app-shared --configuration production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/rollup-starter.git"
  },
  "keywords": [
    "rollup",
    "starter"
  ],
  "author": "beginor <beginor@qq.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beginor/rollup-starter/issues"
  },
  "homepage": "https://github.com/beginor/rollup-starter#readme",
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.2.0",
    "@types/bootstrap": "^5.2.1",
    "bootstrap-icons": "^1.9.1",
    "tslib": "^2.4.0",
    "@angular/animations": "^14.1.1",
    "@angular/common": "^14.1.1",
    "@angular/compiler": "^14.1.1",
    "@angular/core": "^14.1.1",
    "@angular/forms": "^14.1.1",
    "@angular/platform-browser": "^14.1.1",
    "@angular/platform-browser-dynamic": "^14.1.1",
    "@angular/router": "^14.1.1",
    "@angular/cdk": "^14.1.1",
    "@angular/material": "^14.1.1",
    "rxjs": "^7.5.6",
    "zone.js": "~0.11.7",
    "@angular/localize": "^14.1.1",
    "devextreme": "^22.1.4",
    "devextreme-angular": "^22.1.4",
    "@ng-bootstrap/ng-bootstrap": "^13.0.0",
    "ng-zorro-antd": "^13.4.0"
  },
  "devDependencies": {
    "eslint": "^8.21.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "rollup": "^2.77.2",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-esbuild": "^4.9.3",
    "sass": "^1.54.3",
    "typescript": "^4.7.4",
    "esbuild": "^0.14.53",
    "@angular-devkit/build-angular": "^14.1.1",
    "@angular/cli": "^14.1.1",
    "@angular/compiler-cli": "^14.1.1",
    "ng-packagr": "^14.1.0"
  }
}
